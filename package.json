{
  "name": "kp-deep",
  "version": "1.0.0",
  "scripts": {
    "dev": "concurrently \"npm:dev-main\" \"npm:dev-slides\" \"npm:dev-demo\"",
    "dev-main": "vite --port 3000",
    "dev-slides": "vite --port 3001 --config vite.slides.config.js",
    "dev-demo": "vite --port 3002 --config vite.demo.config.js",
    "build": "npm run build-main && npm run build-slides && npm run build-demo",
    "build-main": "vite build",
    "build-slides": "vite build --config vite.slides.config.js",
    "build-demo": "vite build --config vite.demo.config.js",
    "preview": "concurrently \"npm:preview-main\" \"npm:preview-slides\" \"npm:preview-demo\"",
    "preview-main": "vite preview --port 3000",
    "preview-slides": "vite preview --port 3001 --config vite.slides.config.js",
    "preview-demo": "vite preview --port 3002 --config vite.demo.config.js"
  },
   "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    
    "concurrently": "^9.2.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "vite": "^5.0.8"
  }
}
